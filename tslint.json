{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rulesDirectory": [
    "src"
  ],
  "rules": {
    "no-console": false,
    "interface-name": false,
    "member-access": false,
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false,
    "max-classes-per-file": [
      false
    ],
    "array-type": [
      false
    ],
    "max-line-length": [
      true,
      150
    ],
    "variable-name": false,
    "no-shadowed-variable": false
  }
}
